<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="product_pricelist_view_inherit_xas_stock_extend" model="ir.ui.view">
        <field name="name">product.pricelist.form.inherit.xas.stock.extend</field>
        <field name="model">product.pricelist</field>
        <field name="inherit_id" ref="product.product_pricelist_view"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='pricelist_settings']" position="after">
                <group name="default">
                    <field name="default" string="Por defecto"/>
                </group>
            </xpath>
        </field>
    </record>

</odoo>